<div id="User_Login_Form">
	<form action="/login/" method="post" enctype="application/x-www-form-urlencoded">
		<input class="input_text" type="text" tabindex="1" id="username" name="username" placeholder="E-Mail" required><br>
		<input class="input_text" type="Password" tabindex="2" id="userpass" name="userpass" placeholder="Passwort" required><br>

		<input type="submit" name="submit" id="user_login_submit" tabindex="3" value="Anmelden" class="button">
	</form>
</div>
